@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>自动打包</title>
    <link href="~/Scripts/easyui/themes/color.css" rel="stylesheet" />
    <link href="~/Scripts/easyui/themes/icon.css" rel="stylesheet" />
    <link href="~/Scripts/easyui/themes/default/easyui.css" rel="stylesheet" />
    <script src="~/Scripts/easyui/jquery.min.js"></script>
    <script src="~/Scripts/easyui/jquery.easyui.min.js"></script>
    <script src="~/Scripts/easyui/autopack.js"></script>
</head>
<body>
    <div>
        <div class="easyui-window" title="console" style="width:800px;height:400px;padding:5px;">
            <div class="easyui-layout" data-options="fit:true">
                <div id="output" data-options="region:'west'" style="width:100%"></div>
                <div data-options="region:'south',border:false" style="text-align:center;padding:5px 0 0;">
                    <a id="next" class="easyui-linkbutton" style="width:80px">下一步</a>
                    <a id="prev" class="easyui-linkbutton" style="width:80px">前一步</a>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
